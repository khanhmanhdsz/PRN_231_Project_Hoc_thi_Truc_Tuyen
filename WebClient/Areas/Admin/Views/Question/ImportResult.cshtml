﻿@{
    string importTime = Context.Request.Query["importTime"];
    string importResult = Context.Request.Query["importResult"];
    string quizId = Context.Request.Query["quizId"];
}

<div class="pagetitle">
    <h1>View import question result</h1>
</div><!-- End Page Title -->
<div class="card p-4">
    <div class="mb-3">
        <h5>@importTime</h5>
        <h5>@importResult</h5>
    </div>
    <form asp-area="Admin" asp-controller="Question" asp-action="ExportFailedRecordsToFile" method="post">
        <a class="btn btn-secondary" asp-area="Admin" asp-controller="Quiz" asp-action="Details" asp-route-quizId="@quizId">
            <i class="bi bi-arrow-return-left"></i>
            <span>Back</span>
        </a>
        <button type="submit" class="btn btn-primary mx-2">
            <i class="bi bi-box-arrow-up"></i>
            <span>Export failed records</span>
        </button>
    </form>
</div>